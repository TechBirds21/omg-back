  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Premium handcrafted saree e-commerce platform",
  "author": "Techbirds Consulting <info@techbirdsconsulting.com>",
  "homepage": "https://omaguva.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/techbirdsconsulting/omaguva-ecommerce"
  },
  "keywords": [
    "ecommerce",
    "sarees",
    "fashion",
    "react",
    "typescript",
    "omaguva",
    "handcrafted",
    "premium"
